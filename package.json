{
  "name": "bindl",
  "description": "Downloads binaries directly or from archives as defined in a config file",
  "version": "0.0.0-development",
  "author": "Felipe Santos @felipecrs",
  "bin": {
    "bindl": "lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felipecrs/bindl.git"
  },
  "bugs": {
    "url": "https://github.com/felipecrs/bindl/issues"
  },
  "dependencies": {
    "@felipecrs/decompress-tarxz": "^5.0.4",
    "@xhmikosr/decompress-tar": "^8.1.0",
    "@xhmikosr/decompress-tarbz2": "^8.1.0",
    "@xhmikosr/decompress-targz": "^8.1.0",
    "@xhmikosr/decompress-unzip": "^7.1.0",
    "@xhmikosr/downloader": "^15.2.0",
    "chalk": "^5.5.0",
    "clipanion": "^4.0.0-rc.4",
    "cosmiconfig": "^9.0.0",
    "execa": "^9.6.0",
    "listr2": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.17.0",
    "c8": "^10.1.3",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9.32.0",
    "eslint-plugin-unicorn": "^60.0.0",
    "installed-check": "^9.3.0",
    "neostandard": "^0.12.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.7",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "zx": "^8.7.2"
  },
  "engines": {
    "node": "^20.5.0 || >=22.0.0"
  },
  "files": [
    "/lib"
  ],
  "homepage": "https://github.com/felipecrs/bindl#readme",
  "license": "MIT",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "rimraf lib && tsc && rimraf --glob \"lib/!(*index).d.ts\"",
    "format": "prettier --write .",
    "lint": "tsc --noEmit && eslint . && prettier --check . && installed-check --ignore-dev",
    "prepack": "npm run build",
    "test": "c8 vitest --no-watch",
    "posttest": "npm run lint"
  },
  "volta": {
    "node": "22.18.0",
    "npm": "11.5.2"
  }
}
