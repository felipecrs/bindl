{
  "name": "bindl",
  "description": "Downloads and extracts binaries from compressed packages using a config file",
  "version": "2.0.0",
  "author": "Felipe Santos @felipecrs",
  "bin": {
    "bindl": "./bin/run"
  },
  "bugs": "https://github.com/felipecrs/bindl/issues",
  "dependencies": {
    "@felipecrs/decompress-tarxz": "^4.0.0",
    "@oclif/core": "^1.9.1",
    "@oclif/plugin-version": "^1.1.0",
    "chalk": "^4.1.2",
    "cosmiconfig": "^7.0.1",
    "decompress-tar": "^4.1.1",
    "decompress-tarbz2": "^4.1.1",
    "decompress-targz": "^4.1.1",
    "decompress-unzip": "^4.0.1",
    "download": "^8.0.0",
    "listr": "^0.14.3",
    "pkginfo": "^0.4.1",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/download": "^8.0.1",
    "@types/jest": "^28.1.1",
    "@types/listr": "^0.14.4",
    "@types/node": "^10.17.60",
    "@types/shelljs": "^0.8.11",
    "@types/tmp": "^0.2.3",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-prettier": "^8.5.0",
    "execa": "^5.1.1",
    "jest": "^28.1.1",
    "nyc": "^15.1.0",
    "oclif": "^3.0.1",
    "prettier": "^2.7.0",
    "semantic-release": "^19.0.3",
    "shx": "^0.3.4",
    "tmp": "^0.2.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "engines": {
    "node": ">= 14"
  },
  "files": [
    "/bin/run*",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/felipecrs/bindl#readme",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "bindl",
    "default": ".",
    "commands": "./lib",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "plugins": [
      "@oclif/plugin-version"
    ]
  },
  "repository": "felipecrs/bindl",
  "scripts": {
    "build": "shx rm -rf lib/ && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "test": "nyc jest",
    "posttest": "npm run lint",
    "version": "oclif readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "volta": {
    "node": "16.16.0",
    "npm": "8.14.0"
  }
}
