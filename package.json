{
  "name": "bindl",
  "description": "Downloads and extracts binaries from compressed packages using a config file",
  "version": "0.1.0",
  "author": "Felipe Santos @felipecassiors",
  "bin": {
    "bindl": "./bin/run"
  },
  "bugs": "https://github.com/felipecassiors/bindl/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "chalk": "^4.1.0",
    "cosmiconfig": "^7.0.0",
    "decompress-tar": "^4.1.1",
    "decompress-tarbz2": "^4.1.1",
    "decompress-targz": "^4.1.1",
    "decompress-tarxz": "^3.0.0",
    "decompress-unzip": "^4.0.1",
    "download": "^8.0.0",
    "listr": "^0.14.3",
    "pkginfo": "^0.4.1",
    "shelljs": "^0.8.4",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@oclif/dev-cli": "^1.22.2",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/download": "^6.2.4",
    "@types/listr": "^0.14.2",
    "@types/node": "^10.17.28",
    "@types/shelljs": "^0.8.8",
    "@types/tmp": "^0.2.0",
    "commitizen": "^4.1.5",
    "conventional-changelog-conventionalcommits": "^4.4.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^6.11.0",
    "execa": "^4.0.3",
    "get-bin-path": "^5.1.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "semantic-release": "^17.1.1",
    "tmp": "^0.2.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/felipecassiors/bindl",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "bindl"
  },
  "repository": "felipecassiors/bindl",
  "scripts": {
    "posttest": "eslint . --ext .ts",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "nyc jest",
    "version": "oclif-dev readme && git add README.md",
    "release": "semantic-release",
    "cz": "cz"
  },
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "oclif-dev readme && git add README.md",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
